import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    Scanner s=new Scanner(System.in);
            int S,l,b;
    float r;
        S=s.nextInt();
        l=s.nextInt();
        b=s.nextInt();
        r=s.nextFloat();
    int r1=S*S;
    System.out.println(r1);
    int r2=l*b;
    System.out.println(r2);
    double r3= 3.14*r*r;
    System.out.printf("%.2f",r3);
    }
}
